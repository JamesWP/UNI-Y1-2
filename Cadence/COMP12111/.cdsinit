let( (bindKeyFileList file path saveSkillPath)
    bindKeyFileList = '( 
                 "schBindKeys.il"
                    )
    
;  this is the path that is searched for the files
    path = strcat(
              ".   ~   "
              prependInstallPath("local ")
              prependInstallPath("samples/local")
             )
    saveSkillPath=getSkillPath()
    setSkillPath(path)
;
;
 
