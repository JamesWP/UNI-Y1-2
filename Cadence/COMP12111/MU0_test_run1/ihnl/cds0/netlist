// Library - COMP12111, Cell - halfadder, View - schematic
// LAST TIME SAVED: Oct 10 15:59:24 2013
// NETLIST TIME: Dec 12 15:47:27 2013
`timescale 1ns / 10ps 

module halfadder ( carry, sum, a, b );

output  carry, sum;

input  a, b;


specify 
    specparam CDS_LIBNAME  = "COMP12111";
    specparam CDS_CELLNAME = "halfadder";
    specparam CDS_VIEWNAME = "schematic";
endspecify

AND2  I1 ( .O(sum), .I1(net11), .I0(net12));
AND2  I0 ( .O(carry), .I1(b), .I0(a));
OR2  I2 ( .O(net11), .I1(b), .I0(a));
INV  I3 ( .O(net12), .I(carry));

endmodule
