// Library - COMP12111, Cell - adder_4bit, View - schematic
// LAST TIME SAVED: Oct 17 15:23:53 2013
// NETLIST TIME: Dec 12 15:47:27 2013
`timescale 1ns / 10ps 

module adder_4bit ( cout, s, a, b, cin );

output  cout;

input  cin;

output [3:0]  s;

input [3:0]  a;
input [3:0]  b;


specify 
    specparam CDS_LIBNAME  = "COMP12111";
    specparam CDS_CELLNAME = "adder_4bit";
    specparam CDS_VIEWNAME = "schematic";
endspecify

fulladder I3 ( cout, s[3], a[3], b[3], net14);
fulladder I2 ( net14, s[2], a[2], b[2], net16);
fulladder I1 ( net16, s[1], a[1], b[1], net18);
fulladder I0 ( net18, s[0], a[0], b[0], cin);

endmodule
